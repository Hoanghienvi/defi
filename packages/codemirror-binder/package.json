{
  "name": "codemirror-binder",
  "version": "1.3.8",
  "description": "CodeMirror binder for defi.js",
  "main": "index.js",
  "scripts": {
    "test": "npm run cover",
    "cover": "../../node_modules/.bin/babel-node ../../node_modules/.bin/babel-istanbul cover test/index.js",
    "unit": "../../node_modules/.bin/babel-node test/index.js",
    "npm-compile": "../../node_modules/.bin/babel src.js --out-file npm/index.js && cp package.json npm/package.json && cp README.md npm/README.md",
    "build": "../../node_modules/.bin/webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finom/defi.git"
  },
  "keywords": [
    "defi",
    "codemirror"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/defi/issues"
  },
  "homepage": "https://github.com/finom/defi/tree/master/packages/codemirror-binder",
  "dependencies": {
    "@babel/runtime": "^7.10.2"
  },
  "devDependencies": {
    "codemirror": "^5.53.2",
    "defi": "^1.3.8"
  },
  "peerDependencies": {
    "codemirror": "*",
    "defi": "*"
  },
  "gitHead": "5d73b7d6892730283893fe296dea35cdef74f461"
}
